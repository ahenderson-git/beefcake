{
  "productName": "beefcake",
  "version": "0.3.0",
  "identifier": "au.com.ahenderson.beefcake",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:14206",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "./dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "beefcake",
        "url": "/",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "docs/*",
      "data/pipelines/templates/*"
    ],
    "windows": {
      "nsis": {
        "installerIcon": "icons/icon.ico"
      },
      "wix": {
        "upgradeCode": "6dd52fb7-441f-46fe-bdb6-f949f5016261"
      }
    }
  }
}
